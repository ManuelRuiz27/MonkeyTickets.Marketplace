{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "rootDir": "..",
    "testEnvironment": "node",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
        "^@monomarket/contracts$": "<rootDir>/../../packages/contracts/src",
        "^@monomarket/config$": "<rootDir>/../../packages/config/src"
    }
}